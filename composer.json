{
    "name": "gosuperscript/phpstan-rules",
    "type": "phpstan-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Superscript\\PHPStanRules\\": "src/"
        },
        "files": [
            "src/basepath.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Superscript\\PHPStanRules\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "php": "^8.3",
        "phpstan/phpstan": "^2.1",
        "azjezz/psl": "^3.3 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "phpstan/extension-installer": "^1.4",
        "php-standard-library/phpstan-extension": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
