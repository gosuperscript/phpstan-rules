{
    "name": "gosuperscript/phpstan-rules",
    "type": "phpstan-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Superscript\\PHPStanRules\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superscript\\PHPStanRules\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "php": "^8.3",
        "phpstan/phpstan": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    }
}
